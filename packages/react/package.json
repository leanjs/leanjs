{
  "name": "@leanjs/react",
  "version": "0.15.2",
  "author": "Alex Lobera <alex@leanjs.com>",
  "description": "Create your own React bindings for LeanJS Runtime",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leanjs/leanjs.git",
    "directory": "packages/react"
  },
  "files": [
    "dist",
    "18.d.ts",
    "runtime"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./18": {
      "types": "./dist/18.d.ts",
      "default": "./dist/18.js"
    }
  },
  "devDependencies": {
    "@leanjs/core": "^0.22.1",
    "@testing-library/react": "^12.1.4",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/react": "^17.0.41",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "wait-for-expect": "^3.0.2"
  },
  "peerDependencies": {
    "@leanjs/core": "^0.13.4",
    "react": "^17.0.2"
  },
  "scripts": {
    "prepublishOnly": "cp ./dist/18.d.ts . && mkdir runtime && cp -R ./dist/runtime/*.d.ts ./runtime",
    "test": "jest",
    "prebuild": "del-cli dist",
    "build": "tsc --outDir dist --project ./tsconfig.build.json"
  },
  "keywords": [
    "react",
    "microfrontends",
    "modular monolith",
    "micro-frontends"
  ],
  "publishConfig": {
    "access": "public"
  }
}
