{
  "name": "@leanjs/react",
  "version": "0.19.0",
  "author": "Alex Lobera <alex@leanjs.com>",
  "description": "Create your own React bindings for LeanJS Runtime",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leanjs/leanjs.git",
    "directory": "packages/react"
  },
  "files": [
    "dist",
    "18.d.ts",
    "runtime"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./18": {
      "types": "./dist/18.d.ts",
      "default": "./dist/18.js"
    },
    "./17": {
      "types": "./dist/17.d.ts",
      "default": "./dist/17.js"
    },
    "./16": {
      "types": "./dist/16.d.ts",
      "default": "./dist/16.js"
    }
  },
  "typesVersions": {
    "*": {
      "16": [
        "dist/16.d.ts"
      ],
      "17": [
        "dist/17.d.ts"
      ],
      "18": [
        "dist/18.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@leanjs/core": "^0.25.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.4",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/react": "^17.0.41",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "wait-for-expect": "^3.0.2"
  },
  "peerDependencies": {
    "@leanjs/core": "^0.13.4",
    "react": "^17.0.2"
  },
  "scripts": {
    "test": "jest",
    "prebuild": "del-cli dist",
    "build": "tsc --outDir dist --project ./tsconfig.build.json"
  },
  "keywords": [
    "react",
    "microfrontends",
    "modular monolith",
    "micro-frontends"
  ],
  "publishConfig": {
    "access": "public"
  }
}
